{
  "SourceControlOptions": [
    {
      "FileName": "DataModelSchema",
      "PropertiesToRemove": [ "createdTimestamp", "lastUpdate", "lastSchemaUpdate", "lastProcessed", "modifiedTime", "structureModifiedTime", "refreshedTime", "state", "connectionString" ],
      "ExportDaxToFile": true,
      "AddFileExtension": ".json",
      "IsJsonFile":  true
    },
    {
      "FileName": "Report\\Layout",
      "PropertiesToRemove": [ "query", "dataTransforms" ],
      "PropertiesToExpand": [ "config", "filters" ],
      "AddFileExtension": ".json",
      "IsJsonFile": true
    },
    {
      "FileName": "DiagramState",
      "AddFileExtension": ".json",
      "IsJsonFile": true
    },
    {
      "FileName": "SecurityBindings",
      "DeleteFile": true
    },
    {
      "FileName": "Report\\LinguisticSchema",
      "DeleteFile": true
    },
    {
      "FileName": "DataMashup",
      "TreatAsArchive": true,
      "ArchiveDestinationPath": "DataMashupSourceData"
    }
  ],
  "PowerBITemplatePath": ".\\PowerBITemplatePath.pbit",
  "PowerBISourceControlPath": ".\\PowerBISourcePath"

}
